{
  "name": "Keycloak",
  "version": "1.0.0",
  "slug": "keycloak",
  "description": "Keycloak Identity Provider with MariaDB integration",
  "arch": ["amd64", "armv7", "aarch64"],
  "startup": "application",
  "boot": "auto",
  "map": ["ssl"],
  "ports": { "8080/tcp": 8080 },
  "options": {
    "db_name": "keycloak",
    "db_user": "keycloak",
    "db_password": "secret",
    "KC_HOSTNAME": "",
    "KEYCLOAK_ADMIN": "admin",
    "KEYCLOAK_ADMIN_PASSWORD": "admin"
  },
  "schema": {
    "db_name": "str",
    "db_user": "str",
    "db_password": "str",
    "KC_HOSTNAME": "str",
    "KEYCLOAK_ADMIN": "str",
    "KEYCLOAK_ADMIN_PASSWORD": "str"
  },
  "services": ["mysql:need"],
  "ingress": true,
  "ingress_port": 8080,
  "panel_icon": "mdi:openid",
  "panel_title": "Keycloak",
  "init": false
}