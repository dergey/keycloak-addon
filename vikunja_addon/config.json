{
  "name": "Vikunja",
  "version": "0.24.6",
  "slug": "vikunja",
  "description": "Vikunja — менеджер задач, интегрированный в Home Assistant через Ingress",
  "arch": ["amd64", "armv7", "aarch64"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "ingress": true,
  "ingress_port": 3456,
  "ingress_entry": "/",
  "panel_icon": "mdi:format-list-checkbox",
  "panel_title": "Vikunja",
  "ports": { "3456/tcp": null },
  "map": ["data:rw"],
  "options": {
    "db_user": "vikunja",
    "db_password": "supersecret",
    "db_name": "vikunja",
    "jwt_secret": "changeme",
    "allow_register": true
  },
  "schema": {
    "db_user": "str",
    "db_password": "str",
    "db_name": "str",
    "jwt_secret": "str",
    "allow_register": "bool"
  },
  "services": ["mysql:need"],
  "build_from": {
    "amd64": "ghcr.io/home-assistant/amd64-base:latest",
    "aarch64": "ghcr.io/home-assistant/aarch64-base:latest",
    "armv7": "ghcr.io/home-assistant/armv7-base:latest"
  }
}
