{
  "name": "Keycloak",
  "version": "22.0.5",
  "slug": "keycloak",
  "description": "Keycloak с интеграцией MariaDB Home Assistant",
  "arch": ["amd64", "armv7", "aarch64"],
  "startup": "application",
  "boot": "auto",
  "ports": {
    "8080/tcp": 8080
  },
  "ports_description": {
    "8080/tcp": "Keycloak Web-интерфейс"
  },
  "options": {
    "KEYCLOAK_ADMIN": "admin",
    "KEYCLOAK_ADMIN_PASSWORD": "changeme",
    "KC_HOSTNAME": "homeassistant.local",
    "DB_HOST": "core-mariadb",
    "DB_PORT": 3306,
    "DB_NAME": "keycloak",
    "DB_USER": "keycloak",
    "DB_PASSWORD": "your_password"
  },
  "schema": {
    "KEYCLOAK_ADMIN": "str",
    "KEYCLOAK_ADMIN_PASSWORD": "str",
    "KC_HOSTNAME": "str?",
    "DB_HOST": "str",
    "DB_PORT": "int",
    "DB_NAME": "str",
    "DB_USER": "str",
    "DB_PASSWORD": "str"
  },
  "map": ["ssl"],
  "init": false,
  "host_network": false,
  "network": "bridge"
}